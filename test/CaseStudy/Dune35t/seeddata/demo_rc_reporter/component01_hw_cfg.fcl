daq: {
 
  max_fragment_size_words: 262144
 
  fragment_receiver: {
  
    mpi_buffer_count: 16
    first_event_builder_rank: PLACEHOLDER
    event_builder_count: PLACEHOLDER

    generator: ToySimulator
      fragment_type: TOY1
      fragment_id: 0
      board_id: 0

      throw_exception: true

    sleep_on_stop_us: 0
      nADCcounts: 40
      throttle_usecs: 100000
      random_seed: 4556
  }

    metrics: {
    brRunControl: {
      metricPluginType: "RCReporter"
      level: 1
      fileName: "/data/lbnedaq/daqlogs/boardreader/br_%UID%_metrics.log"
      uniquify: true
    }
  }

}